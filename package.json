{
  "name": "new-tab",
  "displayName": "New tab",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build --hoist",
    "package": "plasmo package"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@notionhq/client": "^2.3.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tippyjs/react": "^4.2.6",
    "@tiptap/extension-code-block-lowlight": "^2.26.1",
    "@tiptap/extension-task-item": "^2.26.1",
    "@tiptap/extension-task-list": "^2.26.1",
    "@tiptap/pm": "^2.26.1",
    "@tiptap/react": "^2.26.1",
    "@tiptap/starter-kit": "^2.26.1",
    "@types/lodash": "^4.17.20",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "liquid-glass-react": "^1.1.1",
    "lodash": "^4.17.21",
    "lowlight": "^3.3.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.6.0",
    "three": "^0.179.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "patch-package": "^8.0.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "3.2.4",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://api.notion.com/*"
    ],
    "permissions": [
      "storage",
      "favicon"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; style-src 'self' 'unsafe-inline'"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "assets/backgrounds/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}
